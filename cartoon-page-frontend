   <script>
    async function loadVideos() {
        try {
            const response = await fetch('http://s3backpart2-377407161.us-east-2.elb.amazonaws.com/videos');  #your backend code 
            const videoUrls = await response.json();

            const container = document.getElementById('videos');
            container.innerHTML = '';

            // Array of unique thumbnails ‚Äî make sure this matches or exceeds video count
            const thumbnailUrls = [
                'https://i.ytimg.com/vi/ldDuVSre5Xk/hq720.jpg?sqp=-oaymwEhCK4FEIIDSFryq4qpAxMIARUAAAAAGAElAADIQj0AgKJD&rs=AOn4CLCz1nxfK1OU2lRJiyPIoM5jU2W3wQ', # your thmailnail images 
                'https://m.media-amazon.com/images/M/MV5BZjE4YzViZmEtYjRhMi00NzIyLTkxZDktMzQzM2IzNTA5MWQ2XkEyXkFqcGc@._V1_.jpg',
                'https://i.ytimg.com/vi/LCm55hv9ZZI/hq720.jpg?sqp=-oaymwEhCK4FEIIDSFryq4qpAxMIARUAAAAAGAElAADIQj0AgKJD&rs=AOn4CLDvQ4B35VXvY7Tg_X6aWLKYH5l64g',
                'https://i.ytimg.com/vi/dWtJ6jpCEsM/hq720.jpg?sqp=-oaymwEhCK4FEIIDSFryq4qpAxMIARUAAAAAGAElAADIQj0AgKJD&rs=AOn4CLAxZ2YiMCeccxTUHG6KyJ0H97O6dQ',
                'https://m.media-amazon.com/images/S/pv-target-images/259fbc97ce839d091caea10e1d0c626a4821eb5f7086568e4b1b61a8f9036252._SX1080_FMjpg_.jpg',
                'https://i.ytimg.com/vi/t0Q2otsqC4I/hq720.jpg?sqp=-oaymwEhCK4FEIIDSFryq4qpAxMIARUAAAAAGAElAADIQj0AgKJD&rs=AOn4CLBgbkdHwq9v7C3DObsH54uBSf8hiw',
                'https://filmschoolrejects.com/wp-content/uploads/2021/02/Tom-Jerry-Best-Cartoons-HBO-Max-800x480.jpg',
                'https://www.deccanchronicle.com/h-upload/2024/02/09/1074665-tomandjerrycaughtintheact.webp',
                'https://i.ytimg.com/vi/JTeWMEB5Ssk/hq720.jpg?sqp=-oaymwEhCK4FEIIDSFryq4qpAxMIARUAAAAAGAElAADIQj0AgKJD&rs=AOn4CLD2iONweZbA1KWWTqMdvSNTG4VZKw' // ‚úÖ New thumbnail
            ];

            videoUrls.forEach((url, index) => {
                console.log('Creating video box for:', url); // Debug

                const box = document.createElement('div');
                box.className = 'video-box';

                // Thumbnail
                const thumbnail = document.createElement('img');
                thumbnail.className = 'thumbnail';
                thumbnail.src = thumbnailUrls[index % thumbnailUrls.length]; // Loop if fewer thumbs
                thumbnail.alt = `Video ${index + 1}`;

                // Video
                const video = document.createElement('video');
                video.src = url;
                video.id = `video-${index}`;
                video.controls = true;
                video.preload = "auto";
                video.style.display = 'none';

                // Info
                const videoInfo = document.createElement('div');
                videoInfo.className = 'video-info';
                videoInfo.innerHTML = `
                    <h3>Song - ${index + 1}</h3>
                    <p>Artist Name ‚Ä¢ ${Math.floor(Math.random() * 1000)}K views ‚Ä¢ ${Math.floor(Math.random() * 12) + 1} months ago</p>
                `;

                // Toggle playback on thumbnail click
                thumbnail.onclick = () => {
                    document.querySelectorAll('video').forEach(v => {
                        if (v !== video) {
                            v.pause();
                            v.style.display = 'none';
                            v.parentElement.querySelector('.thumbnail').style.display = 'block';
                        }
                    });

                    if (video.paused) {
                        video.style.display = 'block';
                        thumbnail.style.display = 'none';
                        video.play();
                    } else {
                        video.pause();
                        video.style.display = 'none';
                        thumbnail.style.display = 'block';
                    }
                };

                box.appendChild(thumbnail);
                box.appendChild(video);
                box.appendChild(videoInfo);
                container.appendChild(box);
            });
        } catch (error) {
            console.error('Error loading videos:', error);
            alert("Failed to load videos.");
        }
    }

    window.onload = loadVideos;
</script>


    </body>
